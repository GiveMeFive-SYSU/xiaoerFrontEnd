<view class="main">
    <view class="header">
        <image src="{{userInfo.avatarUrl}}" class="thumb"></image>
        <text class="nickname">{{userInfo.nickName}}</text>
        <text class="about">{{name}}</text>
    </view>
    <view class="record">
    <text space="emsp">今日数据  </text>
    <text>营业额:{{money}}</text>
    <text space="emsp">   点单数:{{cases}}</text>
    </view>
    <view class="directory">
        <view class="direc-entry">
            <navigator url="{{paysetting}}">收支表</navigator>
        </view>
        <view class="direc-entry">
            <navigator url="{{tablesetting}}">桌位管理</navigator>
        </view>
        <view class="direc-entry">
            <navigator url="{{menusetting}}">菜单设置</navigator>
        </view>
        <view class="direc-entry">
          <navigator url="{{queuesetting}}">排队管理</navigator>
        </view>
         <view class="direc-entry">
            <navigator url="{{shopnamesetting}}">更改店铺名</navigator>
        </view>
    </view>

  <swiper catchtap='onSwiperTap' indicator-dots='true' autoplay='true' interval='5000'>
    <swiper-item>
      <image src='/images/1.jpg' data-postid="0"></image>
    </swiper-item>
    <swiper-item>
      <image src='/images/2.jpg' data-postid="1"></image>
    </swiper-item>
    <swiper-item>
      <image src='/images/3.jpg' data-postid="2"></image>
    </swiper-item>
  </swiper>

</view>