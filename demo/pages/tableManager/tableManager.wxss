/* tableManager.wxss */

page {
  height: 100%;
}

.table-list {
  height: 70%;
}

.newest-list {
  display: inline-block;
  width: 120rpx;
  height: 120rpx;
  margin: 14rpx;
  border-radius: 50%;
  text-align: center;
  color: #ff9e01;
  border: 1px solid #ff9e01;
}

.newest-list:nth-child(5n) {
  margin-right: 0;
}

.navi {
  display: inline-block;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}

.newest-list .newest-text {
  font-size: 32rpx;
  margin-top: 37rpx;
  text-align: center;
}

.name-input {
  display: inline-block;
  width: 400rpx;
}

.add-button {
  border: none;
  display: inline-block;
  width: 200rpx;
  font-size: 24rpx;
  position: absolute;
  left: 450rpx;
  color: white;
  background: #ff9e01;
}

.add-table {
  border: 1px solid #ff9e01;
  border-radius: 10px;
  font-size: 24rpx;
  position: absolute;
  left: 50rpx;
}

.submit-button {
  position: absolute;
  bottom: 13%;
  background: #ff9e01;
  color: white;
  width: 50%;
  margin-left: 25%;
  border-radius: 187.5rpx;
  height: 8%;
  font-size: 36rpx;
}
